{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.company.com/propertyNew.schema.json",
  "type": "object",
  "properties": {
    "street": {
      "type": "string",
      "minLength": 1,
      "maxLength": 100
    },
      "city": {
      "type": "string",
      "minLength": 1,
      "maxLength": 100
    },
      "state": {
      "type": "string",
      "minLength": 1,
      "maxLength": 100
    },
      "zip": {
      "type": "integer",
      "minLength": 1,
      "maxLength": 100
    },
    "property_type": {
      "type": "integer" 
    },
    "cost": {
      "type": "integer",
      "minimum": 0
    },
    "description": {
      "type": "string",
      "minLength": 1,
      "maxLength": 150
    },
    "property_owner": {
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": [
    "street",
    "city",
    "state",
    "zip",
    "property_owner",
    "property_type"
  ]
}
